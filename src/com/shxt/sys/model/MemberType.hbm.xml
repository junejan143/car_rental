<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.shxt.sys.model">
    <class name="MemberType" table="web_sys_membertype">
        <id name="type_id" type="integer">
            <generator class="native"></generator>
        </id>
    
        <property name="type_name" type="string" length="32"/>
        <property name="type_status" type="string" length="2"/>
        <property name="discount" type="string" length="10"/>
        <property name="photo" type="string" length="100"/>
        
        
    </class>




</hibernate-mapping>
